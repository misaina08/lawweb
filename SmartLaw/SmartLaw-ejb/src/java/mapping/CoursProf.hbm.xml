<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="modele.CoursProf" table="COURSPROF" lazy="false">
	    <id name="idEmp" column="IDEMP" type="string">
                <generator class="assigned"/>  
            </id>
	    <property name="idCours" column="IDCOURS"/>
            <property name="dateCours" column="DATECOURS" type="date"/>
            <property name="heureDebut" column="HEUREDEBUT"/>
            <property name="heureFin" column="HEUREFIN"/>

	</class>
</hibernate-mapping>
