<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="modeles.parametres.ModeFacturation" table="modefacturation" lazy="false">
	    <id name="id" column="idmode" type="integer">
                
                <generator class="sequence">  
                    <param name="sequence">modefacturation_idmode_seq</param>  
                </generator>  
            </id>
	    <property name="libelle" column="libelle"/>
	    <property name="taux" column="taux"/>
            
	</class>
</hibernate-mapping>
