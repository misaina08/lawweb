<hibernate-mapping>
	<class name="modeles.facturation.TarifFactInterv" table="tariffactinterv" lazy="false">
	    <id name="id" column="idtariffactinterv" type="integer">
                
                <generator class="sequence">  
                    <param name="sequence">tariffactinterv_idtariffactinterv_seq</param>  
