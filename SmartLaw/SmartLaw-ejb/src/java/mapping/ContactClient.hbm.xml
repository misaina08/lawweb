<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="modeles.contact.ContactClient" table="contactclient" lazy="false">
	    <id name="id" column="idcontactcl" type="integer">
                
                <generator class="sequence">  
                    <param name="sequence">contactclient_idcontactcl_seq</param>  
                </generator>  
            </id>
	    <property name="idContact" column="idcontact"/>
	    <property name="idClient" column="idclient"/>
	    
	</class>
</hibernate-mapping>
