<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="modeles.evenement.EvtTarif" table="evttarif" lazy="false">
	    <id name="id" column="idevttarif" type="integer">
                
                <generator class="sequence">  
                    <param name="sequence">evttarif_idevttarif_seq</param>  
                </generator>  
            </id>
	    <property name="idTypeTarif" column="idtypetarif"/>
	    <property name="libelle" column="libelle"/>
	    <property name="code" column="code"/>
	    <property name="numero" column="numero"/>
	    <property name="duree" column="duree"/>
	    <property name="montant" column="montant"/>
             <property name="type" column="type"/>
	    
	    
            
	</class>
</hibernate-mapping>
