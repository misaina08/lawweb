<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="modeles.clients.ClientLibelle" table="clientlibelle" lazy="false">
	    <id name="id" column="idclient" type="integer">
                
                <generator class="sequence">  
                    <param name="sequence">client_idclient_seq</param>  
                </generator>  
            </id>
	    <property name="nom" column="nom"/>
            <property name="idTitre" column="idtitre"/>
            <property name="adresse" column="adresse"/>
            <property name="titre" column="libelle"/>
            <property name="codePostal" column="codepostal"/>
            <property name="ville" column="ville"/>
            <property name="pays" column="pays"/>
            <property name="standard" column="standard"/>
            <property name="bureau" column="bureau"/>
            <property name="fax" column="fax"/>
            <property name="email" column="email"/>
            <property name="telex" column="telex"/>
            <property name="web" column="web"/>
            <property name="autre" column="autre"/>
            <property name="mobile" column="mobile"/>
            <property name="dateCreation" column="datecreation"/>
            <property name="dateModification" column="datemodification"/>

	</class>
</hibernate-mapping>
