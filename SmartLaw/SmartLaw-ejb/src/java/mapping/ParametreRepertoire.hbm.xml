<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="modeles.parametres.ParametreRepertoire" table="parametrerepertoires" lazy="false">
	    <id name="id" column="idrepertoire" type="integer">
                
                <generator class="sequence">  
                    <param name="sequence">repertoire_idrepertoire_seq</param>  
                </generator>  
            </id>
	    <property name="chemin" column="chemin"/>
	    <property name="nom" column="nom"/>
	    
	</class>
</hibernate-mapping>
